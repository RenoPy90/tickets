{
    "version": 2,
    "builds": [
      { "src": "api/*.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1.js" },
      { "src": "/index", "dest": "/public/index.html" },
      { "src": "/tickets", "dest": "/public/tickets.html" },
      { "src": "/(.*)", "dest": "/public/$1" }
    ]
  }
  